{
  "ConnectionStrings": {
    "ApplicationContext": "host=localhost;port=5432;database=currencyaggregation;user id=postgres;password=<secretpassword>"
  },
  "CurrConvHttpSettings": {
    "Url": "https://free.currconv.com/api/v7/convert?q={currConvCurrencyPairTypes}&compact=ultra&apiKey=<secretapikey>"
  },
  "ApplicationSettings": {
    "CurrencyAggregationBackgroundWorkerPeriodicity": 1,
    "CurrencyAggregationBackgroundWorkerEnabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
